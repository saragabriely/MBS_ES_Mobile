<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Filantroplanta.Views.Componentizacao.LabelListaVazia"
             x:Name="ControleLabelListaVazia"
             BindingContext="{x:Reference ControleLabelListaVazia}"
             >
    
    <VerticalStackLayout>
        
        <Label Text="{Binding TextLabel}" FontSize="14" TextColor="DarkGray" HorizontalTextAlignment="Center" Margin="0,20,0,0" />
        
    </VerticalStackLayout>
    
</ContentView>
